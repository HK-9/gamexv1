                                    datasets: [{
                                        label: 'Delivered',
                                        data: [
                                         '{{eachDaySale.[0].total}}',
                                        '{{eachDaySale.[1].total}}',
                                        '{{eachDaySale.[2].total}}',
                                        '{{eachDaySale.[3].total}}',
                                        '{{eachDaySale.[4].total}}',
                                        '{{eachDaySale.[5].total}}',
                                        '{{eachDaySale.[6].total}}',         
